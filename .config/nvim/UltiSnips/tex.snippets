endsnippet
########################################
snippet bin "math binom" i
\binom{$1}{$2}
endsnippet
########################################
snippet binq "math qbinom" i
\qbinom{$1}{$2}
endsnippet
########################################
snippet qrp "math qrisingpower" i
\qrisingpower{$1}
endsnippet
########################################
snippet qfp "math qfallingpower" i
\qfallingpower{$1}
endsnippet
########################################
snippet iset "inline set" i
\\${3:left}\\{\\${6:left.} $1 \\: \\${5:right}| \\: $2 \\${4:right}\\}
endsnippet
########################################
snippet mset "math set" i
\\${3:left}\\{\\${6:left.}
	$1
		\\: \\${5:right}| \\:
	$2
\\${4:right}\\}
endsnippet
########################################
snippet lim "math lim" i
\lim_{$1\to $2}
endsnippet
########################################
snippet | "math restriction" i
\\${2:Bigg}|_{$1}
endsnippet
########################################
snippet \\ "align new row" i
\\\\
&= 
endsnippet
########################################
snippet ul "underline" i
\underline{$1}
endsnippet
########################################
snippet te "insert text" i
\text{$1}
endsnippet
########################################
snippet bf "insert text bold font" i
\textbf{$1}
endsnippet
########################################
snippet sc "insert text smallcaps font" i
\textsc{$1}
endsnippet
########################################
snippet textsc "insert text smallcaps font" i
\textsc{$1}
endsnippet

