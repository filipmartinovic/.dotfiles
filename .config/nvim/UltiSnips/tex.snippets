########################################
snippet beg "\begin{ }" i
\begin{${1:equation*}}
    $2
	${3:\text.}
\end{$1}
endsnippet
########################################
snippet equ "\begin{equation*}" b
\begin{equation*}
    $1
	${3:\text.}
\end{equation*}
endsnippet
########################################
snippet ali "\begin{align*}" b
\begin{align*}
    $1
	${3:\text.}
\end{align*}
endsnippet
########################################
snippet the "\begin{theorem}" b
\begin{theorem}
    $1
\end{theorem}


endsnippet
########################################
snippet cor "\begin{corollary}" b
